{
  "rules": {
    "arrow-parens": true,
    "class-name": true,
    "eofline": true,
    "forin": true,
    "indent": [true, "spaces"],
    "label-position": true,
    "label-undefined": true,
    "linebreak-style": [true, "LF"],
    "no-any": false,
    "no-arg": true,
    "no-bitwise": true,
    "no-console": false,
    "no-construct": true,
    "no-conditional-assignment": true,
    "no-debugger": false,
    "no-duplicate-variable": true,
    "no-eval": true,
    "no-inferrable-types": true,
    "no-internal-module": true,
    "no-invalid-this": [true, "check-function-in-method"],
    "no-reference": true,
    "no-require-imports": true,
    "no-shadowed-variable": true,
    "no-string-literal": true,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": true,
    "no-unreachable": true,
    "no-unsafe-finally": true,
    "no-unused-expression": true,
    "no-unused-new": true,
    "no-unused-variable": [true, "react"],
    "no-use-before-declare": true,
    "no-var-keyword": true,
    "no-var-requires": true,
    "one-variable-per-declaration": true,
    "quotemark": [true, "single"],
    "radix": true,
    "semicolon": [true, "never"],
    "switch-default": true,
    "trailing-comma": [true, {"multiline": "never", "singleline": "never"}],
    "triple-equals": [true, "allow-null-check"],
    "typedef": [false, "call-signature", "parameter"],
    "use-isnan": true,
    "use-strict": [true, "check-module"],
    "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore", "allow-trailing-underscore"],
    "whitespace": [false, "check-branch", "check-decl", "check-operator", "check-module", "check-separator", "check-type", "check-typecast"]
  }
}