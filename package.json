{
  "name": "beta-orionis",
  "version": "0.1.0",
  "description": "A server for space games.",
  "main": "dist/bundle.js",
  "scripts": {
    "build": "npm run clean && npm run lint && ./node_modules/.bin/tsc",
    "clean": "rm -rf ./dist",
    "lint": "./node_modules/.bin/tslint ./src/index.ts ./src/index.test.ts ./example/index.example.ts",
    "start": "npm run build && node ./dist/src/index.js",
    "test": "npm run build && ./node_modules/.bin/mocha --timeout 10000 --colors ./dist/src/index.test.js",
    "example": "npm run build && node ./dist/example/index.example.js"
  },
  "keywords": [
    "Space Center"
  ],
  "author": "Jason Andrew Hills",
  "dependencies": {
    "express": "^4.14.0",
    "lodash": "^4.16.6",
    "node-uuid": "^1.4.7",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "karma": "^1.3.0",
    "mocha": "^3.1.2",
    "tslint": "^3.15.1",
    "typescript": "^2.0.7",
    "typings": "^1.5.0"
  }
}
